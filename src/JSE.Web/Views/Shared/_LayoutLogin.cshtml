<!DOCTYPE html>
<html lang="pt-br">
<head>
    <title>JSE - Administração</title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link href="~/favicon.png" rel="icon" type="image/png" />
    <link href="~/admin/css/bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="~/admin/css/fonts/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/admin/css/animate/animate.css" rel="stylesheet" />
    <link href="~/admin/css/css-hamburgers/hamburgers.min.css" rel="stylesheet" />
    <link href="~/admin/css/select2/select2.min.css" rel="stylesheet" />
    <link href="~/admin/css/login/main.css" rel="stylesheet" />
    <link href="~/admin/css/login/util.css" rel="stylesheet" />

</head>
<body>

    @RenderBody()


    <script src="~/admin/js/vendor/jquery/jquery-3.2.1.min.js"></script>
    <script src="~/admin/js/vendor/bootstrap/popper.min.js"></script>
    <script src="~/admin/js/vendor/bootstrap/bootstrap.min.js"></script>
    <script src="~/admin/js/select2/select2.min.js"></script>
    <script src="~/admin/js/tilt/tilt.jquery.min.js"></script>
    <script>
        $('.js-tilt').tilt({
            scale: 1.1
        })
    </script>

    <script src="~/js/login/main2.js"></script>


    <script type="text/javascript">

        $(document).ready(function () {
            TriggerAlertClose();
        });

        function TriggerAlertClose() {
            $('.alert').fadeIn(1000);
            setTimeout(function () {
                $('.alert').fadeOut(1000);
            }, 5000);
        }
    </script>



    @RenderSection("Scripts", required: false)

</body>
