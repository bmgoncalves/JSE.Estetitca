    <meta content="" name="descriptison">
    <meta content="" name="keywords">

    <!-- Favicons -->
    <link href="~/client/img/favicon.png" rel="icon">
    <link href="~/client/img/apple-touch-icon.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
